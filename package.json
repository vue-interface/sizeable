{
  "name": "@vue-interface/sizeable",
  "version": "1.0.1",
  "description": "The sizeable mixin.",
  "files": [
    "dist"
  ],
  "main": "./dist/sizeable.umd.js",
  "module": "./dist/sizeable.es.js",
  "exports": {
    ".": {
      "import": "./dist/sizeable.es.js",
      "require": "./dist/sizeable.umd.js"
    }
  },
  "browserslist": "last 2 versions, > 0.5%, ie >= 11",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "pre-release": "npm run build; git add . -A; git commit -m 'pre-release commit'",
    "release-patch": "npm run pre-release && npm version patch -m \"%s\" && npm run release;",
    "release-minor": "npm run pre-release && npm version minor -m \"%s\" && npm run release;",
    "release-major": "npm run pre-release && npm version major -m \"%s\" && npm run release;",
    "release": "git add . -A; git commit; git push --tags origin; npm publish;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-interface/sizeable.git"
  },
  "keywords": [
    "Sizeable",
    "Vue",
    "Mixin",
    "Tailwind",
    "Bootstrap"
  ],
  "author": "Justin Kimbrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vue-interface/sizeable/issues"
  },
  "homepage": "https://github.com/vue-interface/sizeable/docs#readme",
  "dependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "change-case": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "pascalcase": "^2.0.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "vite": "^2.7.2",
    "vite-plugin-vue2": "^1.9.3"
  }
}
